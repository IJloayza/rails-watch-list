<h1>Movies</h1>
<P><%= @list.name %></p>
<% @list.bookmarks.each do |bookmark| %>
  <%= bookmark.comment %>
  <%= link_to 'Delete', bookmark_path(bookmark),method: :delete,data: { confirm: "Are you sure?" }, class: 'btn btn-primary' %>
<% end %>
<ul>
<% Movie.all.each do |movie|%>
  <li><%= movie.title %></li>
  <li><%= movie.overview%>></li>
<% end %>
</ul>
<%= link_to 'Add a new comment', "#{@list.id}/bookmarks/new", class: 'btn btn-primary' %>
<%= link_to 'Back', lists_path , class: 'btn btn-primary' %>
